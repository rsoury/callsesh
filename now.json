{
	"version": 2,
	"env": {
		"SESSION_SECRET": "@app-session-secret",
		"TWILIO_ACCOUNT_SID": "@twilio-account-sid",
		"TWILIO_AUTH_TOKEN": "@twilio-auth-token",
		"TWILIO_PROXY_SERVICE_SID": "@twilio-proxy-service-sid",
		"AUTH0_CLIENT_SECRET": "@auth0-client-secret",
		"STRIPE_SECRET_KEY": "",
		"AWS_ACCESS_KEY_ID": "@aws-access-key-id",
		"AWS_SECRET_ACCESS_KEY": "@aws-access-secret-key"
	},
	"build": {
		"env": {
			"PUBLIC_URL": "https://callsesh.now.sh",
			"SESSION_SECRET": "@app-session-secret",
			"TWILIO_ACCOUNT_SID": "@twilio-account-sid",
			"TWILIO_AUTH_TOKEN": "@twilio-auth-token",
			"TWILIO_PROXY_SERVICE_SID": "@twilio-proxy-service-sid",
			"AUTH0_DOMAIN": "callsesh.auth0.com",
			"AUTH0_CLIENT_ID": "@auth0-client-id",
			"AUTH0_CLIENT_SECRET": "@auth0-client-secret",
			"STRIPE_PUBLIC_KEY": "",
			"STRIPE_SECRET_KEY": "",
			"SENTRY_DSN": "@sentry-dsn",
			"SENTRY_ORG": "web-doodle",
			"SENTRY_PROJECT": "callsesh",
			"SENTRY_AUTH_TOKEN": "@sentry-auth-token",
			"AWS_ACCESS_KEY_ID": "@aws-access-key-id",
			"AWS_SECRET_ACCESS_KEY": "@aws-access-secret-key"
		}
	},
	"builds": [
		{
			"src": "packages/app/next.config.js",
			"use": "@now/next"
		}
	],
	"rewrites": [
		{
			"source": "/auth/(.*)",
			"destination": "http://callsesh-auth.s3-website.us-east-2.amazonaws.com/$1"
		},
		{
			"source": "^/favicon.ico",
			"destination": "/packages/app/static/favicon/favicon.ico"
		},
		{
			"source": "(.*)",
			"destination": "/packages/app/$1"
		}
	]
}
