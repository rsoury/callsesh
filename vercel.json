{
	"version": 2,
	"builds": [
		{
			"src": "packages/app/next.config.js",
			"use": "@now/next"
		}
	],
	"rewrites": [
		{
			"source": "/auth/(.*)",
			"destination": "http://callsesh-auth.s3-website.us-east-2.amazonaws.com/$1"
		},
		{
			"source": "^/favicon.ico",
			"destination": "/packages/app/static/favicon/favicon.ico"
		},
		{
			"source": "(.*)",
			"destination": "/packages/app/$1"
		}
	]
}
