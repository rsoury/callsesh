		"country-list": "^2.2.0",
		"formik": "^2.1.4",
		"formik-wizard": "^3.1.2",
		"is-empty": "^1.2.0",
		"libphonenumber-js": "^1.7.50",
		"lodash": "^4.17.15",
		"next": "latest",
		"next-connect": "^0.6.4",
		"next-seo": "^4.4.0",
		"passport": "^0.4.1",
		"passport-custom": "^1.1.1",
		"passport-remember-me": "^0.0.1",
