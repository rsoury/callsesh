		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.19.0",
		"husky": "^4.2.5",
		"is-ci-cli": "^2.0.0",
		"jest": "^25.4.0",
		"lint-staged": "^10.1.7",
		"prettier": "^2.0.5",
		"react-test-renderer": "^16.13.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"./**/*.+(js|jsx)": [
			"yarn lint --fix"
		],
		"./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"yarn format"
		]
	}
}
